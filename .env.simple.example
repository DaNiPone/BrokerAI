# Ejemplo de archivo .env para BrokerAI (configuración simple)
# Renombra este archivo a .env y configura tus valores

# Credenciales de MySQL (para la configuración simple)
MYSQL_ROOT_PASSWORD_SIMPLE=your_mysql_root_password_simple
MYSQL_DATABASE_SIMPLE=wordpress
MYSQL_USER_SIMPLE=wordpress
MYSQL_PASSWORD_SIMPLE=your_mysql_password_simple

# Credenciales de WordPress para MySQL (para la configuración simple)
WORDPRESS_DB_HOST_SIMPLE=mysql
WORDPRESS_DB_USER_SIMPLE=wordpress
WORDPRESS_DB_PASSWORD_SIMPLE=your_wordpress_db_password_simple
WORDPRESS_DB_NAME_SIMPLE=wordpress

# Configuración de WordPress (para la configuración simple)
WP_MEMORY_LIMIT_SIMPLE=256M
WP_BROKER_WEBHOOK_URL_SIMPLE=http://n8n-main:5678/webhook/broker-ai/webhook
WP_BROKER_WEBHOOK_SECRET_SIMPLE=your_broker_webhook_secret_simple

# Configuración de n8n (para la configuración simple)
N8N_HOST_SIMPLE=localhost
N8N_PORT_SIMPLE=5678
N8N_PROTOCOL_SIMPLE=http
N8N_EDITOR_BASE_URL_SIMPLE=http://localhost:5678
WEBHOOK_URL_SIMPLE=http://localhost:5678
WORDPRESS_URL_SIMPLE=http://wordpress-main
WEBHOOK_SECRET_SIMPLE=your_webhook_secret_simple
N8N_DIAGNOSTICS_ENABLED_SIMPLE=false
N8N_USER_MANAGEMENT_DISABLED_SIMPLE=true
EXECUTIONS_PROCESS_SIMPLE=main
GENERIC_TIMEZONE_SIMPLE=America/Mexico_City
TZ_SIMPLE=America/Mexico_City

# Credenciales de phpMyAdmin (si decides usarlo en desarrollo)
PMA_HOST_SIMPLE=mysql
PMA_USER_SIMPLE=root
PMA_PASSWORD_SIMPLE=your_phpmyadmin_password_simple
