# Ejemplo de archivo .env para BrokerAI
# Renombra este archivo a .env y configura tus valores

# Credenciales de MySQL
MYSQL_ROOT_PASSWORD=your_mysql_root_password
MYSQL_DATABASE=broker_main
MYSQL_USER=broker_user
MYSQL_PASSWORD=your_mysql_password

# Credenciales de WordPress para MySQL
WORDPRESS_DB_HOST=db
WORDPRESS_DB_USER=broker_user
WORDPRESS_DB_PASSWORD=your_wordpress_db_password
WORDPRESS_DB_NAME=broker_main
WORDPRESS_DB_NAME_AGENTS=broker_agents
WORDPRESS_DB_NAME_CLIENTS=broker_clients

# Credenciales de PostgreSQL para n8n
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=your_n8n_postgres_password

# Credenciales de PostgreSQL (servicio postgres)
POSTGRES_USER=n8n
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=n8n

# Credenciales de phpMyAdmin
PMA_HOST=db
PMA_USER=root
PMA_PASSWORD=your_phpmyadmin_password

# URLs de WordPress
WP_HOME_MAIN=http://localhost:8000
WP_SITEURL_MAIN=http://localhost:8000
WP_HOME_AGENTS=http://localhost:8001
WP_SITEURL_AGENTS=http://localhost:8001
WP_HOME_CLIENTS=http://localhost:8002
WP_SITEURL_CLIENTS=http://localhost:8002

# Configuraci√≥n de n8n
N8N_WEBHOOK_URL=http://n8n:5678/
WEBHOOK_SECRET=your_webhook_secret
N8N_WORDPRESS_URL=http://wordpress-main
N8N_WORDPRESS_USER=admin
N8N_WORDPRESS_APP_PASSWORD=your_wordpress_app_password
MAIN_SITE=http://localhost:8000
AGENTS_PATH=/agentes
CLIENTS_PATH=/clientes
